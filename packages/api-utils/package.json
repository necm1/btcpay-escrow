{
  "name": "@escrow/api-utils",
  "version": "0.0.1",
  "private": true,
  "main": "./src/index.js",
  "module": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "default": "./src/index.js"
    }
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "dist/packages/api-utils",
          "tsConfig": "packages/api-utils/tsconfig.lib.json",
          "packageJson": "packages/api-utils/package.json",
          "main": "packages/api-utils/src/index.ts",
          "assets": [
            "packages/api-utils/*.md"
          ]
        }
      }
    }
  }
}
