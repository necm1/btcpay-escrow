{
  "name": "@escrow/orm",
  "version": "0.0.1",
  "private": true,
  "main": "./src/index.js",
  "module": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "default": "./src/index.js"
    }
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "pg": "^8.16.3",
    "tslib": "^2.3.0",
    "typeorm": "^0.3.26"
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "dist/packages/orm",
          "tsConfig": "packages/orm/tsconfig.lib.json",
          "packageJson": "packages/orm/package.json",
          "main": "packages/orm/src/index.ts",
          "assets": [
            "packages/orm/*.md"
          ]
        }
      }
    }
  },
  "devDependencies": {
    "@types/pg": "^8"
  }
}
